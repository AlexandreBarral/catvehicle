<!-- This file launches sysid node generated using sysid_cat.slx file-->

<launch>

    <param name="enable_statistics" value="true" />
    <arg name="leader_robot" default="catvehicle"/>
    <arg name="vel" default="2.0"/> <!-- 35.5 m/s is upper limit in the velocity-->
    <arg name="accel" default="0"/>
    <arg name="str" default="0.14"/>
    <arg name="strate" default="0"/>

    <group ns="$(arg leader_robot)">
        <param name="init_vel" value="$(arg vel)"/>
        <param name="vel_slope" value="$(arg accel)"/>
        <param name="init_str" value="$(arg str)"/>
        <param name="str_slope" value="$(arg strate)"/>
        <node pkg="sysid_cat" type="sysid_cat_node" name="sysid_cat_$(arg leader_robot)" output="screen" required="true"/>
    </group>

</launch>
